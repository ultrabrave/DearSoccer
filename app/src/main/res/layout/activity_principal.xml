<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorWhite"
    tools:context=".views.listMatchs.principal.PrincipalActivity">
    <LinearLayout
        android:id="@+id/ac_princi_lineaprincipal"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:weightSum="100"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="13"
            android:orientation="horizontal"
            android:weightSum="100"
            android:background="@color/colorWhite">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="11"></LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="10"
                android:orientation="vertical"
                android:weightSum="100">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="25"></LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="46"
                    android:background="@drawable/btnconfig"
                    android:clickable="true"
                    android:id="@+id/btnconfigprincipal"
                    >
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="27"></LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="5"></LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="50"
                android:orientation="vertical"
                android:weightSum="100">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="23"></LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="60"
                    android:background="@drawable/logofinal"
                    >
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="25"></LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="10"
                android:orientation="vertical"
                android:weightSum="100">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="25"></LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:orientation="horizontal"
                    android:layout_weight="46"
                    android:background="@drawable/btnconfigadd"
                    android:clickable="true"
                    android:id="@+id/btnaddconfigprincipal"
                    >
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="27"></LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="15"></LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ac_princi_18"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="0"
            android:background="@android:color/transparent"
            android:orientation="horizontal"
            android:tileMode="repeat">
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LineaViewPager"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="72"
            android:background="@android:color/white"
            android:orientation="horizontal"
            android:tileMode="repeat" >
            <android.support.v4.view.ViewPager
                android:layout_height="fill_parent"
                android:layout_width="wrap_content"
                android:id="@+id/viewPager">
            </android.support.v4.view.ViewPager>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="15"
            android:orientation="vertical"
            android:tileMode="repeat"
            android:weightSum="100">
            <android.support.design.widget.AppBarLayout
                android:layout_height="0dp"
                android:layout_weight="100"
                android:layout_width="match_parent"
                android:weightSum="100"
                >
                <android.support.design.widget.TabLayout
                    android:id="@+id/tabLayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:backgroundTint="@color/colorBlueDark"
                    app:tabTextAppearance="@style/tamaÃ±o_texto_iconos"
                    android:background="@color/colorBlueDark"
                    app:tabIndicatorColor="@color/colorBlueDark"
                    app:tabTextColor="@color/colorWhite"
                    android:textAlignment="center"
                    app:tabSelectedTextColor="@color/colorPrimaryDark">
                    <android.support.design.widget.TabItem
                        android:icon="@drawable/searchblack"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        app:theme="@style/AppTheme.ActionBar"
                        android:clickable="true"
                        android:text="Partidos"
                        android:layout_weight="33" />
                    <android.support.design.widget.TabItem
                        android:icon="@drawable/mundog"
                        android:layout_width="0dp"
                        android:clickable="true"
                        android:text="Creados"
                        android:layout_height="match_parent"
                        android:layout_weight="33"/>
                    <android.support.design.widget.TabItem
                        android:icon="@drawable/mundog"
                        android:layout_width="0dp"
                        android:text="Participando"
                        app:theme="@style/AppTheme.ActionBar"
                        android:clickable="true"
                        android:layout_height="match_parent"
                        android:layout_weight="33"/>
                </android.support.design.widget.TabLayout>
            </android.support.design.widget.AppBarLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>